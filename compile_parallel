./simulate_para

g++ -pg -O3 -fno-strict-aliasing -std=c++11 -Wall -Wextra -msse2 -DHAVE_SSE2=1  -DDSFMT_MEXP=19937 -o simulate_opt D2d.cpp dSFMT*/dSFMT.c  -lgsl -lgslcblas -lm -fopenmp -lpthread
./simulate_opt C 0.0 10.0 0.5 0.5 0.5 6 0.5 E D2d_test.dat 0.1 0.1 1.0 100
./simulate_opt C 0.0 10.0 0.5 0.5 0.5 6 0.5 E D2d_test.dat 0.6 0.6 1.0 100
./simulate_opt C 0.0 10.0 0.5 0.5 0.5 6 0.5 E D2d_test.dat 1.1 1.1 1.0 100
